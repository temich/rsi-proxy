{
	"port": "8080",

	"rsi": {
		"strip": 8,
		"cache": {
			"root": "./cache",
			"dirlen": 2
		},
		"temp-prefix": "rsi-",
		"link-regex": "(?:(?:href|src|data)=|(?:url|import)\\()[\"']?([^\"'\\)]+)(?:[\"']?|\\)|>| )",
		"ignore-extensions": [".html", ".htm"]
	},

	"redis": {
		"host": "redis.dev",
		"port": 6379,
		"db": 1
	},

	"tracer" : {
		"root": "./logs",
		"level": "log",
		"format": "<{{title}}> {{timestamp}}\n {{message}}\n in {{file}}:{{line}}"
	}
}